{
  "name": "muhammad-umar/ppp-gateway",
  "description": "Purchasing Power Parity (PPP) fair pricing adjustment by countries with Stripe (or any other payment gateway supported by Laravel Cashier) integration powered by data from the World Bank",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Umar",
      "email": "muhammadumarsotvoldiev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0",
    "laravel/cashier": "^16.0",
    "league/iso3166": "^4.3",
    "trustip/trustip": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "MuhammadUmar\\PPPGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MuhammadUmar\\PPPGateway\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MuhammadUmar\\PPPGateway\\PPPGatewayServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "stable"
}
