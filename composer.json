{
  "name": "your-vendor/ppp-stripe",
  "description": "Laravel package for Purchasing Power Parity (PPP) pricing adjustment with Stripe integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "laravel/cashier": "^15.0",
    "league/iso3166": "^4.3",
    "trustip/trustip": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "YourVendor\\PPPStripe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YourVendor\\PPPStripe\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\PPPStripe\\PPPStripeServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "stable"
}
